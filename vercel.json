{
    "version": 2,
    "builds": [
      
      {
        "src": "frontend/mizzou-scheduler/vite.config.js",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }  
      },
     
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ],
   
    "rewrites": [
        {
          "source": "/(.*)",
          "destination": "frontend/mizzou-scheduler/index.html"
        }
      ]
    
  }

