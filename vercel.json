

  {
    "version": 2,
    "builds": [
      {
        "src": "frontend/mizzou-scheduler/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/mizzou-scheduler/dist",
          "outputDirectory": "frontend/mizzou-scheduler/dist"
        }
      },
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
   
      {
        "src": "/[^.]+",
        "dest": "/"
      }
    ]
  }