{
    "version": 2,
    "builds": [
        {
          "src": "frontend/mizzou-scheduler/package.json",
          "use": "@vercel/static-build",
          "config": {
            "distDir": "dist"
            
          }
        }
        ],
    "routes": [
        {
          "src": "/assets/(.*)",
          "headers": {
            "Cache-Control": "public, max-age=31536000, immutable"
          }
        },
        {
          "src": "/(.*)",
          "dest": "/frontend/mizzou-scheduler/dist/index.html"
        }
      ]
}